{
  "name": "graphs-for-js",
  "version": "0.3.0",
  "description": "Some JavaScript implementation of a graph data structure.\nFeatures:\n\t- Insert and remove nodes. \n\t- Connect and disconnect nodes. \n\t- Algorithms for graph structures.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/ayang4114/graphs-for-js",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.test.ts'",
    "coverage": "nyc mocha -r ts-node/register -r source-map-support/register --recursive 'test/**/*.test.ts'",
    "prepare": "tsc",
    "clean": "rm -rf ./dist"
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "author": {
    "name": "Anthony Yang",
    "email": "ayang4114@gmail.com"
  },
  "license": "ISC",
  "keywords": [
    "nodes",
    "graph",
    "vertex",
    "edge",
    "algorithm",
    "data structure"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.12",
    "@types/collections": "^5.0.2",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.2",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "typescript-collections": "^1.3.3"
  }
}
